import java.util.*;
public class Main
{
	public static void main(String[] args) {
	   // String str = "stscsdf";
	   // int s = 0;
	   // int e = str.length()-1;
	   // System.out.print(pal( s, e,str));
	   
	}
	
	static boolean pal(int s,int e,String str){
	   if(s>=e){
	       return true;
	   }
	   if(str.charAt(s)!=str.charAt(e)){
	       return false;
	   }
	  return pal(s+1,e-1,str);
	}
}
